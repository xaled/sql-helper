{
  "output-profile": "sqlite_jdbc",
  "database":"printfeed",
  "database-version":"1",
  "error-profile":"default",
  "root-package":"com.printfeed",
  "classes": [
    {
      "name":"FeedItem",
      "attributes":[
        {"name":"title", "type":"string"},
        {"name":"link", "type":"string"},
        {"name":"desc", "type":"string"},
        {"name":"srcId", "type":"int"},
        {"name":"seen", "type":"bool"},
        {"name":"date", "type":"date"}
      ]
    },
    {
      "name":"FeedSource",
      "attributes":[
        {"name":"name", "type":"string"},
        {"name":"url", "type":"string"},
        {"name":"tags", "type":"string"}
        ]
    }
  ]

}