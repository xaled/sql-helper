package ${model_package};
<%!
    from profiles.filters import upper,upperFirst,lower
%>
<%
    constructargs = "%s %s"%(vars[0]['jtype'], vars[0]['name'].lower())
    for var in vars[1:]:
        constructargs  += ", %s %s"%(var['jtype'], var['name'].lower())
%>
import java.io.Serializable;
{"name":"name", "type":"string"},
        {"name":"url", "type":"string"},
        {"name":"tags", "type":"string"}
        ]
public class ${classname} implements Serializable {
    /**
     * ${classname} generated by xaled/sqlhelper for database ${dbname} version ${dbversion}
     *  https://github.com/xaled/sql-helper.git
     */

    private static final long serialVersionUID = 1L;

    // private variables
    private long id = -1; //Primary key
% for var in vars:
    private ${var['jtype']} ${var['name'] | lower};
% endfor


    // Empty Constructor
    public ${classname}() {

    }



	// Full Constructor
	public ${classname}(${constructargs}) {
% for var in vars:
        this.${var['name']|lower} = ${var['namelow']|lower};
% endfor
	}


	// getting ID
	public long getId() {
		return this.id;

	}

	// setting Id
	public void setId(long id) {
		this.id = id;

	}

% for var in vars:
	// getting ${var['name']}
	public String get${var['name']|upperFirst}() {
		return this.${var['name']|lower};

	}

	// setting ${var['name']}
	public void set${var['name'] | upperFirst}(String ${var['name'] | lower}) {
		this.${var['name'] | lower} = ${var['namelow'] | lower};
	}
% endfor


}

